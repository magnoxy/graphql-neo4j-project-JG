{
  "version": 2,
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node",
          "config": {
              "distDir": "build"
          }
      }
  ],
  "routes": [
      {
          "src": "/graphql(.*)",
          "dest": "src/main.ts",
          "methods": ["GET", "POST", "PUT", "DELETE"]
      }
  ]
}